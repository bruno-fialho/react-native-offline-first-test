{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"],
        "tests": ["tests/*"],
        "@components/*": ["src/components/*"],
        "types/*": ["src/@types/*"],
      },
      "strictPropertyInitialization": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
    }
    
}
